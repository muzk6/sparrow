{
  "name": "muzk6/sparrow",
  "description": "Sparrow Framework",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "muzk6",
      "email": "muzk6x@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "scripts": {
    "post-create-project-cmd": [
      "php create_project.php",
      "php -r 'unlink(\"create_project.php\");'"
    ]
  },
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-redis": "*",
    "ext-pcntl": "*",
    "ext-xdebug": "*",
    "pimple/pimple": "^3.2",
    "duncan3dc/blade": "^4.5",
    "php-amqplib/php-amqplib": "^2.9"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Core\\": "core/"
    }
  },
  "suggest": {
    "yar": "Required to use the Yar",
    "msgpack": "Required to use the msgpack in Yar",
    "tideways_xhprof": "Required to use the tideways_xhprof",
    "swiftmailer/swiftmailer": "Required to use the Swift_Mailer",
    "elasticsearch/elasticsearch": "Required to use the Elasticsearch"
  }
}
